a:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library a.cpp
	./a.out

b:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library b.cpp
	./a.out

c:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library c.cpp
	./a.out

d:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library d.cpp
	./a.out


e:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library e.cpp
	./a.out < sample.txt

ff:
	g++ -std=gnu++17 -Wall -Wextra -O2 -DONLINE_JUDGE -I /opt/boost/gcc/include  -I /opt/ac-library f.cpp
	./a.out

exec:
	./a.out
